extends ../layouts/staffMenu
block content
 
    h1 List Course
    table.table 
     a.btn(href = '/staff/createCourse/' + category) Create new Course
        tr 
            th Course Name
            th 
        each course in courses
            tr
                td= course.courseName
                td
                    a.btn(href='/staff/updateCourse/'+ course.id) Update 
                    a.btn(href='/staff/deleteCourse/'+ course.id) Delete 


